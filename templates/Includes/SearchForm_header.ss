<form $FormAttributes class="form-inline d-none d-md-block header-search float-right" role="search">
    <div class="input-group">
        <input name="Search" aria-label="search" type="text" class="form-control" placeholder="<%t Bambusa_Search.Placeholder "Search..." %>">
        <div class="input-group-append">
            <button type="submit" class="btn btn-secondary"><%t Bambusa_Search.Button "Search" %></button>
        </div>
    </div>
</form>
